{
    "openapi": "3.0.0",
    "info": {
        "title": "Futbol Femeni API Documentation",
        "version": "1.0.0"
    },
    "paths": {
        "/api": {},
        "/api/zones": {
            "get": {
                "tags": [
                    "Zones"
                ],
                "summary": "Obtener todas las zonas",
                "description": "Devuelve una lista paginada de zonas.",
                "operationId": "0f732fe44c20996cd9dd342f722cb156",
                "responses": {
                    "200": {
                        "description": "Lista de zonas devuelta con éxito.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/ZoneResource"
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Zones"
                ],
                "summary": "Crear una nueva zona",
                "description": "Crea una nuevo zona con los datos proporcionados.",
                "operationId": "253822acdf31d7499579454f17b7fc8b",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ZoneStoreRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Zona creada con éxito.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ZoneResource"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/zones/{id}": {
            "get": {
                "tags": [
                    "Zones"
                ],
                "summary": "Obtener un paciente",
                "description": "Devuelve los datos de una zona específico por su ID.",
                "operationId": "9fbd7dbdda1909cd7ebc9bb4167293d9",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID de la zona",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Zona recuperada con éxito.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ZoneResource"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Zona no encontrado."
                    }
                }
            },
            "put": {
                "tags": [
                    "Zones"
                ],
                "summary": "Actualizar una zona",
                "description": "Actualiza los datos de una zona existente.",
                "operationId": "28fa1be86dd47c22a90f9db32cdf9481",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID de la zona",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ZoneUpdateRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Zona actualizada con éxito.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ZoneResource"
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "Zones"
                ],
                "summary": "Eliminar una zona",
                "description": "Elimina una zona específico por su ID.",
                "operationId": "357ad6da21048b1eed885132d85c7c01",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID de la zona",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Zona eliminado con éxito."
                    },
                    "404": {
                        "description": "Zona no encontrada."
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "ZoneStoreRequest": {
                "required": [
                    "name",
                    "description",
                    "location"
                ],
                "properties": {
                    "name": {
                        "description": "Nombre de la zona",
                        "type": "string",
                        "example": "Ronny Feest"
                    },
                    "description": {
                        "description": "Descripcion de la zona",
                        "type": "string",
                        "example": "Veniam neque libero sed sunt aut porro unde."
                    },
                    "location": {
                        "description": "Ubicacion de la zona",
                        "type": "string",
                        "example": "A Coruña"
                    }
                },
                "type": "object"
            },
            "ZoneUpdateRequest": {
                "properties": {
                    "name": {
                        "description": "Nombre de la zona",
                        "type": "string",
                        "maxLength": 255,
                        "example": "Ronny Feest"
                    },
                    "description": {
                        "description": "Descripcion de la zona",
                        "type": "string",
                        "maxLength": 255,
                        "example": "Veniam neque libero sed sunt aut porro unde."
                    },
                    "location": {
                        "description": "Ubicacion de la zona",
                        "type": "string",
                        "maxLength": 255,
                        "example": "A Coruña"
                    }
                },
                "type": "object"
            },
            "ZoneResource": {
                "description": "Respuestas de la zona",
                "properties": {
                    "id": {
                        "description": "ID de la zona",
                        "type": "integer"
                    },
                    "name": {
                        "description": "Nombre de la zona",
                        "type": "string"
                    },
                    "description": {
                        "description": "Descripcion de la zona",
                        "type": "string"
                    },
                    "location": {
                        "description": "Ubicacion de la zona",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "Zone": {
                "description": "Esquema del model Zona",
                "properties": {
                    "name": {
                        "description": "Nombre de la zona",
                        "type": "string"
                    },
                    "description": {
                        "description": "Descripcion de la zona",
                        "type": "string"
                    },
                    "location": {
                        "description": "Ubicacion de la zona",
                        "type": "string"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
        {
            "name": "Zones",
            "description": "Zones"
        }
    ]
}